{{#partial "content"}}
    <section class="display-part">
        <div class="add-ranger">
            <a href="/rangers/new"><button class="btn btn-unique">Add New Ranger</button></a>
        </div>
        <hr>
        <br>
        <h2>LIST OF RANGERS</h2>
        <div class="container-fluid">
        {{#if ranger}}
            <div class="row">
                {{#each ranger}}
                    <div class="card-body">
                        <h5 class="card-title">{{name}}</h5>
                        <h6 class="card-subtitle mb-2 text-muted">Badge Number: {{badge_number}}</h6>
                        <p class="card-text">
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item">Phone Number: {{phone_number}}</li>
                            <li class="list-group-item">Email Address: {{email}}</li>
                        </ul></p>
                    </div>
                {{/each}}
            </div>
        </div>
        {{else}}

        {{/if}}
    </section>
{{/partial}}

{{> layout.hbs}}
